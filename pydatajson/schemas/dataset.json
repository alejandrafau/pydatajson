{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"required": [
		"title",
		"description",
		"publisher",
		"superTheme",
		"distribution",
		"accrualPeriodicity",
		"issued"
	],
	"properties": {
		"publisher": {
			"type": "object",
			"required": ["name"],
			"properties": {
				"name": { "type": "string" },
				"mbox": { "type": "string" }
			}
		},
		"distribution": {
		       "type": "array",
		       "items": { "$ref": "distribution.json" }
		},
		"title": { "type": "string" },
		"description": { "type": "string" },
		"contactPoint": {
			"type": "object",
			"properties": {
				"fn": { "type": "string" },
				"hasEmail": { "type": "string" }
			}
		},
		"superTheme": { "type": "array" },
		"theme": { "type": "array" },
		"keyword": { "type": "array" },
		"accrualPeriodicity": { "type": "string" },
		"issued": { "type": "string" },
		"modified": { "type": "string" },
		"identifier": { "type": "string" },
		"language": { "type": "array" },
		"spatial": { 
			"anyOf": [
				{ "type": "string" },
				{ "type": "array" }
			]
		},
		"temporal": { "type": "string" },
		"landingPage": { "type": "string" },
		"license": { "type": "string" }
	}
}
