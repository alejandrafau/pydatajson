{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"required": [
		"title",
		"description",
		"publisher",
		"superTheme",
		"distribution",
		"accrualPeriodicity",
		"issued"
	],
	"properties": {
		"publisher": {
			"type": "object",
			"required": ["name"],
			"properties": {
				"name": { "type": "string" },
				"mbox": { "$ref": "mixed-types.json#stringOrNull" }
			}
		},
		"distribution": {
		       "type": "array",
		       "items": { "$ref": "distribution.json" }
		},
		"title": { "type": "string" },
		"description": { "type": "string" },
		"issued": { "type": "string" },
		"superTheme": { "type": "array" },
		"accrualPeriodicity": {
			"type": "string",
			"pattern": "^R/P\d+[Y|M|W|D]$"
		},
		"contactPoint": {
			"type": "object",
			"properties": {
				"fn": { "$ref": "mixed-types.json#stringOrNull" },
				"hasEmail": { "$ref": "mixed-types.json#stringOrNull" }
			}
		},
		"theme": { "$ref": "mixed-types.json#arrayOrNull" },
		"keyword": { "$ref": "mixed-types.json#arrayOrNull" },
		"modified": { "$ref": "mixed-types.json#stringOrNull" },
		"identifier": { "type": "string" },
		"language": { "$ref": "mixed-types.json#arrayOrNull" },
		"spatial": { 
			"anyOf": [
				{ "type": "string" },
				{ "type": "array" },
				{ "type": "null" }
			]
		},
		"temporal": { "$ref": "mixed-types.json#stringOrNull" },
		"landingPage": { "$ref": "mixed-types.json#stringOrNull" },
		"license": { "$ref": "mixed-types.json#stringOrNull" }
	}
}
